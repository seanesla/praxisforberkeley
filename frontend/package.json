{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:improved": "playwright test --config=playwright-improved.config.ts",
    "test:e2e:performance": "playwright test --config=playwright-improved.config.ts --project=performance",
    "test:e2e:mobile": "playwright test --config=playwright-improved.config.ts --project=mobile-chrome --project=mobile-safari",
    "test:e2e:a11y": "playwright test --config=playwright-improved.config.ts --project=a11y",
    "test:e2e:smoke": "playwright test --config=playwright-improved.config.ts --grep @smoke",
    "test:e2e:critical": "playwright test --config=playwright-improved.config.ts --grep @critical",
    "test:e2e:shard": "playwright test --config=playwright-improved.config.ts --shard=$SHARD",
    "test:e2e:parallel": "playwright test --config=playwright-improved.config.ts --workers=4",
    "test:e2e:update-snapshots": "playwright test --config=playwright-improved.config.ts --update-snapshots",
    "test:e2e:codegen": "playwright codegen",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e:clean": "rm -rf test-results playwright-report screenshots .auth",
    "test:e2e:manual": "playwright test real-user-journey.spec.ts --headed --reporter=list,html --timeout=300000",
    "test:e2e:manual:debug": "PWDEBUG=1 playwright test real-user-journey.spec.ts --headed",
    "test:sequential": "ts-node e2e/sequential-test-runner.ts",
    "test:sequential:no-pause": "ts-node e2e/sequential-test-runner.ts --no-pause",
    "test:sequential:verbose": "ts-node e2e/sequential-test-runner.ts --verbose",
    "test:visual": "./run-all-tests-visual.sh",
    "test:visual:update": "playwright test e2e/visual-regression/visual-regression.spec.ts --update-snapshots",
    "test:visual:ui": "npx playwright test --config=playwright-visual.config.ts --ui",
    "test:ct": "playwright test -c playwright-ct.config.ts",
    "test:ct:ui": "playwright test -c playwright-ct.config.ts --ui",
    "test:ct:headed": "playwright test -c playwright-ct.config.ts --headed",
    "lighthouse": "lhci autorun",
    "lighthouse:view": "lhci open",
    "perf:analyze": "npm run build && npm run lighthouse",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "test:ci": "npm run lint && npm run typecheck && npm run test:coverage && npm run test:e2e:improved",
    "test:pr": "npm run test:e2e:smoke && npm run test:e2e:critical",
    "test:nightly": "npm run test:e2e:improved --project=chromium --project=firefox --project=webkit",
    "test:release": "npm run test:e2e:improved && npm run test:e2e:mobile && npm run test:e2e:a11y && npm run test:e2e:performance",
    "test:visual:debug": "./run-all-tests-visual.sh --debug",
    "test:visual:smoke": "./run-all-tests-visual.sh --grep @smoke",
    "test:visual:single": "npx playwright test --config=playwright-visual.config.ts --project=chromium-visual",
    "precommit": "npm run lint && npm run typecheck && npm run test:coverage"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-pdf/renderer": "^4.3.0",
    "@react-three/drei": "^10.3.0",
    "@react-three/fiber": "^9.1.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.81.2",
    "@types/d3": "^7.4.3",
    "@types/katex": "^0.16.7",
    "@types/lodash": "^4.17.18",
    "@types/multer": "^1.4.13",
    "@types/react-resizable": "^3.0.8",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.18.1",
    "html2canvas": "^1.4.1",
    "katex": "^0.16.22",
    "lodash": "^4.17.21",
    "lucide-react": "^0.522.0",
    "multer": "^2.0.1",
    "next": "15.3.3",
    "pdf-parse": "^1.1.1",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-highlight-words": "^0.21.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-pdf": "^9.2.1",
    "react-resizable": "^3.0.5",
    "react-syntax-highlighter": "^15.6.1",
    "react-toastify": "^11.0.5",
    "reactflow": "^11.11.4",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.2",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "three": "^0.177.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-highlight-words": "^0.20.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "^3.2.4",
    "axe-playwright": "^2.1.0",
    "critters": "^0.0.23",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^9.1.0",
    "glob": "^11.0.3",
    "jsdom": "^26.1.0",
    "keyv": "^5.3.4",
    "open": "^10.1.2",
    "prettier": "^3.2.5",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0"
  }
}
